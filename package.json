{
  "scripts": {
    "start": "node dist/main/server.js",
    "dev": "tsx watch src/main/server.ts",
    "format": "prettier --write . && eslint src/** --fix && eslint __tests__/** --fix",
    "test": "jest --runInBand --no-cache",
    "test:w": "npm test -- --watch",
    "test:ci": "npm test -- --silent --coverage --bail=false",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:w": "npm run build -- -w",
    "prisma": "prisma generate",
    "prisma:dev": "npm run prisma && prisma migrate dev",
    "prisma:prod": "npm run prisma && prisma migrate deploy",
    "prisma:reset": "prisma migrate reset"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@swc/core": "^1.3.2",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.1.2",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tsx": "^3.8.2",
    "typescript": "^4.6.4"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
