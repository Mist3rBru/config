{
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.labelFormat": "short",
  "workbench.editor.untitled.hint": "hidden",

  "zenMode.hideLineNumbers": false,
  "zenMode.hideTabs": false,

  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.lineHeight": 26,
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.semanticHighlighting.enabled": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.indentation": true,
  "editor.guides.highlightActiveIndentation": "always",
  "editor.guides.bracketPairs": "active",
  "editor.renderLineHighlight": "gutter",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.quickSuggestions": {
    "strings": true
  },

  "javascript.format.enable": true,
  "javascript.preferences.quoteStyle": "single",
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.inlayHints.parameterNames.enabled": "literals",
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "javascript.format.semicolons": "remove",

  "typescript.format.enable": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.parameterNames.enabled": "literals",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "typescript.format.semicolons": "remove",
  "typescript.tsdk": "node_modules\\typescript\\lib",
  
  "typescriptHero.imports.organizeOnSave": false,
  "typescriptHero.imports.organizeSortsByFirstSpecifier": true,
  "typescriptHero.imports.multiLineTrailingComma": false,
  "typescriptHero.imports.multiLineWrapThreshold": 80,
  "typescriptHero.imports.stringQuoteStyle": "'",
  "typescriptHero.imports.insertSemicolons": false,
  "typescriptHero.imports.removeTrailingIndex": true,
  "typescriptHero.imports.grouping": ["Plains", "Modules", "Workspace"],
  "typescriptHero.imports.insertSpaceBeforeAndAfterImportBraces": true,
  "typescriptHero.imports.disableImportRemovalOnOrganize": false,
  "typescriptHero.imports.disableImportsSorting": false,
  "typescriptHero.imports.ignoredFromRemoval": ["react"],

  "prettier.requireConfig": true,
  "prettier.bracketSpacing": true,
  "prettier.bracketSameLine": false,
  "prettier.arrowParens": "avoid",
  "prettier.endOfLine": "crlf",
  "prettier.semi": false,
  "prettier.useTabs": false,
  "prettier.singleQuote": true,
  "prettier.quoteProps": "as-needed",
  "prettier.printWidth": 80,
  "prettier.trailingComma": "none",
  "prettier.jsxSingleQuote": false,
  "prettier.singleAttributePerLine": false,

  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 300,

  "editor.formatOnSave": true,

  "[prisma]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": false,
    "source.sortMembers": false
  },

  "explorer.compactFolders": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,

  "breadcrumbs.enabled": true,

  "emmet.syntaxProfiles": { "javascript": "jsx" },
  "emmet.includeLanguages": { "javascript": "javascriptreact" },

  "screencastMode.onlyKeyboardShortcuts": true,

  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": false,
    "**/.vscode": false
  },

  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    ".prettierrc": "json",
    "*.tsx": "typescriptreact",
    ".env*": "dotenv",
    "*.css": "tailwindcss"
  },
  
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.folders.color": "#6633cc",
  "material-icon-theme.folders.associations": {
    "application": "Api",
    "express": "routes",
    "protocols": "interface",
    "composers": "components",
    "strategy": "context",
    "factory": "generator",
    "adapters": "plugin",
    "slide": "svelte",
    "main": "core",
    "infra": "app",
    "presentation": "api",
    "entities": "class",
    "domain": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe",
    "providers": "include",
    "subscribers": "messages",
    "useCases": "controller",
    "kafka": "supabase",
    "mappers": "meta",
    "_shared": "shared",
    "eslint-config": "tools",
    "kube": "kubernetes",
    "nginx": "terraform"
  },
  "material-icon-theme.files.associations": {
    "vitest.config.ts": "vite",
    "ormconfig.json": "database",
    "*.proto": "3d",
    "*.webpack.js": "webpack",
    "*.build.tsx": "svgo"
  },
  "material-icon-theme.languages.associations": {
    "dotenv": "tune"
  },

  "cSpell.language": "en,pt",
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!c",
    "!cpp",
    "!csharp",
    "!go",
    "!handlebars",
    "!haskell",
    "!jade",
    "!java",
    "!latex",
    "!php",
    "!pug",
    "!python",
    "!restructuredtext",
    "!rust",
    "!scala",
    "!scss"
  ],
  "cSpell.userWords": [
    "acad",
    "brazilian",
    "chakra",
    "clsx",
    "contentist",
    "Contentist",
    "contentists",
    "cread",
    "Cread",
    "decrypter",
    "Ecommerce",
    "Encrypter",
    "esports",
    "fastify",
    "ftec",
    "Ftec",
    "gmail",
    "Graduacao",
    "hasher",
    "lcov",
    "malabi",
    "microservices",
    "middlewares",
    "mockdate",
    "pomodoro",
    "postgres",
    "prefetch",
    "Procs",
    "rocketseat",
    "spotify",
    "suts",
    "testid",
    "uniftec",
    "Untypeable",
    "upsert",
    "usecases",
    "whatsapp",
    "youtube"
  ],

  "security.workspace.trust.untrustedFiles": "newWindow",
  "debug.onTaskErrors": "abort",
  "editor.minimap.size": "fit",

  "gitlens.hovers.currentLine.over": "line",
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.currentLine.scrollable": false,
  "gitlens.hovers.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,

  "editor.linkedEditing": true,
  "git.openRepositoryInParentFolders": "always",
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "window.menuBarVisibility": "compact",
  "colorize.languages": [
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "stylus",
    "xml",
    "svg",
    "react",
    "typescriptreact"
  ],
  "terminal.integrated.env.windows": {},
  "console-ninja.featureSet": "Community",
  "terminal.integrated.env.linux": {},
  "workbench.colorTheme": "Omni",
  "diffEditor.ignoreTrimWhitespace": false,
  
  "jest.rootPath": "",
  "jest.autoRun": "off",
  "tabnine.experimentalAutoImports": true
}